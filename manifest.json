{
  "manifest_version": 3,
  "name": "Inspirational Message Generator Extension",
  "version": "1.0",
  "description": "Generates random inspirational messages",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["style.css", "script.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
